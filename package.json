    "core-js": "^3.6.5",
    "lodash": "^4.17.15",
    "rxjs": "^7.3.0",
    "rxjs-compat": "^6.5.5",
    "toastr": "^2.1.4",
    "webpack-cli": "^4.5.0",
    "zone.js": "^0.11.4"
